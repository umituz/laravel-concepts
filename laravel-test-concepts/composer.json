{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "andreaselia/laravel-api-to-postman": "^1.7",
        "bvanhoekelen/performance": "^2.5",
        "dipeshsukhia/laravel-html-minify": "^1.1",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.4",
        "laravel/tinker": "^2.0",
        "vigneshc91/laravel-test-generator": "^0.0.5"
    },
    "require-dev": {
        "beyondcode/dusk-dashboard": "^2.0",
        "beyondcode/laravel-er-diagram-generator": "^1.5",
        "beyondcode/laravel-self-diagnosis": "^1.5",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.4",
        "imanghafoori/laravel-microscope": "^1.0",
        "knuckleswtf/scribe": "^3.0",
        "laravel/dusk": "^6.15",
        "laravel/ui": "2.3",
        "mockery/mockery": "^1.0",
        "mpociot/laravel-apidoc-generator": "^4.8",
        "mpociot/laravel-test-factory-helper": "^2.1",
        "mtolhuys/laravel-schematics": "^0.10.3",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
