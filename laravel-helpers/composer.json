                "RegexHelper": "LaravelHelper\\Helpers\\RegexHelper",
                "ArrayHelper": "LaravelHelper\\Helpers\\ArrayHelper",
                "DatabaseHelper": "LaravelHelper\\Helpers\\DatabaseHelper",
